<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" prefHeight="354.0" prefWidth="464.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <StackPane prefHeight="265.0" prefWidth="383.0" translateY="12.0">
         <children>
            <TextField prefHeight="50.0" prefWidth="0.0" scaleX="0.6" translateX="60.0" translateY="-90.0" fx:id="firstNum">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <StackPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </StackPane.margin>
            </TextField>
            <TextField scaleX="0.6" translateX="60.0" translateY="-20.0" fx:id="secondNum">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <StackPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </StackPane.margin>
            </TextField>
            <TextField scaleX="0.6" scaleY="2.0" translateX="60.0" translateY="40.0" fx:id="gcdResult" onAction="#computeGCDClick">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </TextField>
            <Button layoutX="10.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="95.0" text="Compute GCD" textAlignment="CENTER" translateX="-125.0" translateY="90.0" fx:id="computeButton" onAction="#computeGCDClick">
               <StackPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </StackPane.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <effect>
                  <InnerShadow>
                     <color>
                        <Color green="0.4302884638309479" blue="1.0" />
                     </color>
                  </InnerShadow>
               </effect></Button>
            <Button cancelButton="true" layoutX="10.0" mnemonicParsing="false" text="Quit" textAlignment="CENTER" translateX="-40.0" translateY="90.0" onAction="#quitClick" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="A:" textAlignment="CENTER" translateX="-165.0" translateY="-90.0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="B:" translateX="-165.0" translateY="-20.0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="The GCD:" textAlignment="CENTER" translateX="-145.0" translateY="40.0" />
         </children></StackPane>
   </children>
