<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <StackPane prefHeight="284.0" prefWidth="424.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Array/String:" translateX="20.0" translateY="-80.0">
         <font>
            <Font name="Arial Bold Italic" size="15.0" />
         </font>
      </Text>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="The permutations:" translateX="20.0" translateY="50.0">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Text>
      <TextField fx:id="generatedArray" accessibleRole="TEXT" alignment="TOP_LEFT" blendMode="MULTIPLY" cache="true" onAction="#generateArray" promptText="...your array/string" scaleX="0.6" style="-fx-scale-x: 0.5; -fx-translate-x: 90; -fx-translate-y: -80; -fx-font-size: 17;">
         <font>
            <Font size="13.0" />
         </font></TextField>
      <Button fx:id="generateArrayButton" mnemonicParsing="false" onAction="#generateArray" style="-fx-translate-x: 200; -fx-translate-y: -115;" text="Generate Array" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <effect>
            <ColorAdjust brightness="-0.05" contrast="0.06" hue="0.86" saturation="-0.54" />
         </effect>
      </Button>
      <Button fx:id="computePermutationsButton" mnemonicParsing="false" onAction="#computePermutations" style="-fx-translate-x: 20; -fx-translate-y: 120;" text="Compute permutations">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" cancelButton="true" mnemonicParsing="false" onAction="#quitButton" style="-fx-translate-x: 180; -fx-translate-y: 120;" text="Quit" />
      <ScrollPane pannable="true" prefHeight="284.0" prefWidth="244.0" style="-fx-scale-x: 0.5; -fx-scale-y: 0.4; -fx-translate-x: 90; -fx-translate-y: 35;">
         <content>
            <TextArea fx:id="printResultsField" prefHeight="283.0" prefWidth="412.0" promptText="...your results">
               <font>
                  <Font name="Arial" size="26.0" />
               </font></TextArea>
         </content>
      </ScrollPane>
      <Button fx:id="generateArrayButton1" layoutX="10.0" layoutY="139.0" mnemonicParsing="false" onAction="#generateArray" style="-fx-translate-x: 300; -fx-translate-y: -115;" text="Generate String" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS">
         <effect>
            <ColorAdjust brightness="-0.05" contrast="0.06" hue="0.86" saturation="-0.54" />
         </effect>
      </Button>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
